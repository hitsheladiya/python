<odoo>
    <record id="view_daily_transaction_wizard_form" model="ir.ui.view">
        <field name="name">daily.transaction.wizard.form</field>
        <field name="model">daily.transaction.wizard</field>
        <field name="arch" type="xml">
            <form string="Daily Transaction Report">
                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                </group>
                <footer>
                    <button string="Print Report" type="object" name="print_report" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
